<koken:include file="inc/components/header.html" />
<article class="top-section">
	<h1 class="page-name">Visar Etiketter...</h1>
</article>
<koken:load>
	<ul class="grid">
    	<koken:loop>
    		<koken:link to="tag" title="Visa allt i  {{ tag.title }}">
		      	<li class="tag-bg">
		      		<koken:load source="tag" filter:tags="{{ tag.title }}">
		   				<koken:img crop="adaptive"/>
		   			</koken:load>
					<span class="tag">{{ tag.counts.total }} x {{ tag.title }}</span>
		      	</li>
	      	</koken:link>
    	</koken:loop>
  	</ul>
<koken:else>
	No tags found.
</koken:load>
